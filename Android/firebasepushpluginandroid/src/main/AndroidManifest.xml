<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.firebasepushpluginandroid">

 <application
         android:label="@string/app_name">

     <service android:name="com.applicaster.firebasepushpluginandroid.services.APMessagingService">
         <intent-filter android:priority="-1">
             <action android:name="com.google.firebase.MESSAGING_EVENT" />
         </intent-filter>
     </service>

     <service android:name="com.applicaster.firebasepushpluginandroid.services.APInstanceIdService">
         <intent-filter android:priority="-1">
             <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
         </intent-filter>
     </service>

 </application>

</manifest>
